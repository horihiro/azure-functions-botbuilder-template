{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.7/MicrosoftTeams.schema.json",
  "manifestVersion": "1.7",
  "version": "{{MANIFEST_VERSION}}",
  "id": "{{MANIFEST_ID}}",
  "packageName": "{{PACKAGE_NAME}}",
  "developer": {
    "name": "{{DEVELOPER_NAME}}",
    "websiteUrl": "{{WEBSITE_URL}}",
    "privacyUrl": "{{PRIVACY_URL}}",
    "termsOfUseUrl": "{{TERMSOFUSE_URL}}"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "{{SHORT_NAME}}",
    "full": "{{FULL_NAME}}"
  },
  "description": {
    "short": "{{SHORT_DESC}}",
    "full": "{{FULL_DESC}}"
  },
  "accentColor": "#FFFFFF",
  "bots": [
    {
      "botId": "{{BOT_ID_I.E._MSAPP_ID}}",
      "scopes": [
        "personal",
        "team"
      ],
      "commandLists": [
        {
          "scopes": [
            "personal",
            "team"
          ],
          "commands": [
            {
              "title": "Hello",
              "description": "Sends a hello message and @mention the sender"
            }
          ]
        }
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": []
}